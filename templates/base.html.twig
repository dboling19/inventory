<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <link rel="icon" type="image/svg" href="{{ asset('build/images/favicon.light.svg') }}"></link>
    <script src="https://kit.fontawesome.com/88e8e8d392.js" crossorigin="anonymous"></script>
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% set route_name = app.request.attributes.get('_route')%}
    {% block stylesheets %}
      {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
      {{ encore_entry_script_tags('app') }}
    {% endblock %}
    <title>{% block title %}{% endblock %}</title>
  </head>
  <body>
    {% block body %}
      <header>
        {{ include('navbar.html.twig') }}
      </header>
      <noscript>Please enable Javascript to use this page.  Most elements require JS to run correctly.</noscript>
      {% block content %}{% endblock %}
      {# {% for label, messages in app.flashes %}
        {% for message in messages %}
          <span class="db-flash-message db-notif db-panel" role="alert">{{ message }}</span>
        {% endfor %} #}
      {# {% endfor %} #}
      <footer class="db-footer">
        {% block footer %}{% endblock %}
      </footer>
    {% endblock %}
  </body>
</html>
